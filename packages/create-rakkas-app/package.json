{
	"name": "create-rakkas-app",
	"version": "0.7.0-next.21",
	"type": "module",
	"files": [
		"dist",
		"templates"
	],
	"bin": {
		"create-rakkas-app": "./index.js"
	},
	"scripts": {
		"build": "tsup && tsx scripts/create-templates.ts",
		"dev": "tsup --watch",
		"prepack": "rm -rf dist && pnpm build",
		"test": "pnpm test:typecheck && pnpm test:lint && pnpm test:package",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist --ignore-pattern templates",
		"test:package": "publint --strict"
	},
	"description": "Generate Rakkas application boilerplate",
	"license": "MIT",
	"repository": "https://github.com/rakkasjs/rakkasjs",
	"keywords": [
		"react",
		"vite",
		"rakkas"
	],
	"devDependencies": {
		"@cyco130/eslint-config": "^3.4.0",
		"@types/inquirer": "^9.0.3",
		"@types/node": "^20.6.5",
		"detype": "^0.6.3",
		"eslint": "^8.50.0",
		"prettier": "^3.0.3",
		"publint": "^0.2.2",
		"tsup": "^7.2.0",
		"tsx": "^3.13.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.5"
	},
	"dependencies": {
		"cac": "^6.7.14",
		"cpr": "^3.0.1",
		"inquirer": "^9.2.11",
		"mkdirp": "^3.0.1",
		"picocolors": "^1.0.0",
		"rimraf": "^5.0.5"
	}
}
